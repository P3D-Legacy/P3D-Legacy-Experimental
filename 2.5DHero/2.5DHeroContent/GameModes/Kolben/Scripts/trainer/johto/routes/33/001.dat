version=2
:if:<register.exists(phone_reg_007)>=true
    @Text.Show(I refreshed in the~Pok√©mon Center and now I~have even more energy.)
    @register.remove(phone_reg_007)
    @register.newtime(phone_reg_time_007,1,day)
    :if:<player.badges>>1
        :if:<register.exists(rematch_007_1)>=true
            @battle.trainer(johto\routes\33\001_2)
        :else
            @register.new(rematch_007_1)
            @battle.trainer(johto\routes\33\001_1)
        :endif
    :else
        @battle.trainer(johto\routes\33\001)
    :endif
:else
    @battle.starttrainer(johto\routes\33\001)
