version=2
:if:<register.exists(dungeonrobotsdefeated)>=False
    @register.new(dungeonrobotsdefeated,int,0)
:endif

:if:<register.exists(dungeonrobots<level.filename>)>=False
    @storage.set(robotnumber,1)
    :select:<level.filename>
        :when:1,2,6,7,8,9,15,16,17,18,22,23
            @storage.set(robotnumber,2)
    :endselect
    @text.show(Click*Creeeeeeak*WHIIIIIIIIIIIIIR)
    :if:<register.value(dungeonrobotsdefeated)><10
        @storage.set(robotnum,0<register.value(dungeonrobotsdefeated)>)
    :else
        @storage.set(robotnum,<register.value(dungeonrobotsdefeated)>)
    :endif
    @battle.trainer(kanto\dungeon\robots\\<math.floor(<register.value(dungeonrobotsdefeated)>/6)>)
    @register.new(dungeonrobots<level.filename>)
    @register.change(dungeonrobotsdefeated,1+<register.value(dungeonrobotsdefeated)>)
    :if:<register.value(dungeonrobotsdefeated)>=24
        @entity.remove(25)
    :endif
    @entity.remove(<level.filename>)
    @sound.play(destroy)
:else
    @text.show(It seems to have no power.)
:endif
:end