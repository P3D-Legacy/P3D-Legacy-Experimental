version=2
:if:<register.exists(olivine_voltorb_trade)>=True
    @Text.show(Thank you very~much for the Krabby.)
:else
    @Text.show(Hi, I'm looking~for this Pok√©mon.*If you have~Krabby, would*you trade it for~my Voltorb?)
    @Options.show(Yes,No)
    :select:<options.result>
        :when:Yes
