    @npc.Turn(2,1)
    @npc.Move(2,3)
    @npc.Turn(2,2)
    @npc.Move(2,4)
    @npc.Turn(1,1)
    @npc.Move(1,4)
    @npc.Turn(1,2)
    @npc.Move(1,4)
    @npc.Turn(0,1)
    @npc.Move(0,5)
    @npc.Turn(0,2)
    @npc.Move(0,4)
    @NPC.remove(0)
    @NPC.remove(1)
    @NPC.remove(2)
    @NPC.unregister(johto\ilex\main.dat|1|position|8,0,30)
    @NPC.unregister(johto\ilex\main.dat|0|position|9,0,30)
    @NPC.unregister(johto\azalea\0.dat|1|position|5,-2,2)
    @NPC.unregister(johto\azalea\0.dat|2|position|5,-2,5)
    :end
:endif

:if:<register.exists(ilex_farfetchd_intro)>=False <and> <register.exists(ilex_farfetchd_complete)>=False <and> <register.exists(rocket_defeat_slowpokewell)>=True
