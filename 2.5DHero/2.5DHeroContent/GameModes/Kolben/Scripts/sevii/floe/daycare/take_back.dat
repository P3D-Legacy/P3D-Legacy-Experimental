version=2
@daycare.clean(2)

:select:<daycare.countpokemon(2)>
    :when:1
        @options.show(<daycare.pokemonname(2,0)> Lv. <daycare.currentlevel(2,0)>,Cancel)
        :select:<options.result>
            :when:<daycare.pokemonname(2,0)> Lv. <daycare.currentlevel(2,0)>
                @text.show(If you want your~<daycare.pokemonname(2,0)> back,~it will cost $<math.int(<daycare.grownlevels(2,0)>+1*100)>.~Would you like it back?)
                @options.show(Yes,No)
                :select:<options.result>
                    :when:Yes
                        :if:<pokemon.count><6
                            :if:<player.money>><math.int(<daycare.grownlevels(2,0)>+1*100-1)>
                                @player.addmoney(-<math.int(<daycare.grownlevels(2,0)>+1*100)>)
                                @npc.turn(0,3)
                                @level.wait(30)
                                @npc.turn(0,1)
                                @level.wait(30)
                                @npc.turn(0,0)
                                @npc.move(0,1)
                                @sound.play(enter)
                                @level.wait(100)
                                @npc.turn(0,2)
                                @sound.play(enter)
                                @npc.move(0,1)
                                @text.show(Perfect!~Here's your Pokémon.)
                                @pokemon.cry(<daycare.pokemonid(2,0)>)
                                @text.show(<player.name> got~<daycare.pokemonname(2,0)> back~from the Day-Care Lady.*Very well.~Please come again.)
                                @daycare.takepokemon(2,0)
                            :else
                                @text.show(You don't have enough~money to get~<daycare.pokemonname(2,0)> back.)
                            :endif
                        :else
                            @text.show(You don't have room~in your party!*Come back when you've~made room.)
                        :endif
                    :when:No
                        @text.show(Fine.~Come Again.)
                :endselect

            :when:Cancel
                @text.show(Fine.~Come Again.)
        :endselect

    :when:2
        @options.show(<daycare.pokemonname(2,0)> Lv. <daycare.currentlevel(2,0)>,<daycare.pokemonname(2,1)> Lv. <daycare.currentlevel(2,1)>,Cancel)
        :select:<options.result>
            :when:<daycare.pokemonname(2,0)> Lv. <daycare.currentlevel(2,0)>
                :if:<daycare.grownlevels(2,0)>=0
                    @text.show(If you want your~<daycare.pokemonname(2,0)> back,~it will cost $<math.int(<daycare.grownlevels(2,0)>+1*100)>.~Would you like it back?)
                :else
                    @text.show(If you want your~<daycare.pokemonname(2,0)> back,~it will cost $<math.int(<daycare.grownlevels(2,0)>+1*100)>.~Would you like it back?)
                :endif

                @options.show(Yes,No)
                :select:<options.result>
                    :when:Yes
                        :if:<pokemon.count><6
                            :if:<player.money>><math.int(<daycare.grownlevels(2,0)>+1*100-1)>
                                @player.addmoney(-<math.int(<daycare.grownlevels(2,0)>+1*100)>)
                                @npc.turn(0,3)
                                @level.wait(30)
                                @npc.turn(0,1)
                                @level.wait(30)
                                @npc.turn(0,0)
                                @npc.move(0,1)
                                @sound.play(enter)
                                @level.wait(100)
                                @npc.turn(0,2)
                                @sound.play(enter)
                                @npc.move(0,1)
                                @text.show(Perfect!~Here's your Pokémon.)
                                @pokemon.cry(<daycare.pokemonid(2,0)>)
                                @text.show(<player.name> got~<daycare.pokemonname(2,0)> back~from the Day-Care Lady.)
                                @daycare.takepokemon(2,0)

                                @text.show(Would you like to have~the other one back, too?)
                                @options.show(Yes,No)
                                :select:<options.result>
                                    :when:Yes
                                        @script.start(sevii\floe\daycare\take_back)
                                    :when:No
                                        @text.show(Very well.~Please come again.)
                                :endselect
                            :else
                                @text.show(You don't have enough~money to get~<daycare.pokemonname(2,0)> back.)
                            :endif
                        :else
                            @text.show(You don't have room~in your party!*Come back when you've~made room.)
                        :endif
                    :when:No
                        @text.show(Fine.~Come Again.)
                :endselect
            :when:<daycare.pokemonname(2,1)> Lv. <daycare.currentlevel(2,1)>
            
                :if:<daycare.grownlevels(2,1)>=0
                    @text.show(If you want your~<daycare.pokemonname(2,1)> back,~it will cost $<math.int(<daycare.grownlevels(2,1)>+1*100)>.~Would you like it back?)
                :else
                    @text.show(If you want your~<daycare.pokemonname(2,1)> back,~it will cost $<math.int(<daycare.grownlevels(2,1)>+1*100)>.~Would you like it back?)
                :endif

                @options.show(Yes,No)
                :select:<options.result>
                    :when:Yes
                        :if:<pokemon.count><6
                            :if:<player.money>><math.int(<daycare.grownlevels(2,1)>+1*100-1)>
                                @player.addmoney(-<math.int(<daycare.grownlevels(2,1)>+1*100)>)
                                @npc.turn(0,3)
                                @level.wait(30)
                                @npc.turn(0,1)
                                @level.wait(30)
                                @npc.turn(0,0)
                                @npc.move(0,1)
                                @sound.play(enter)
                                @level.wait(100)
                                @npc.turn(0,2)
                                @sound.play(enter)
                                @npc.move(0,1)
                                @text.show(Perfect!~Here's your Pokémon.)
                                @pokemon.cry(<daycare.pokemonid(2,1)>)
                                @text.show(<player.name> got~<daycare.pokemonname(2,1)> back~from the Day-Care Lady.)
                                @daycare.takepokemon(2,1)

                                @text.show(Would you like to have~the other one back, too?)
                                @options.show(Yes,No)
                                :select:<options.result>
                                    :when:Yes
                                        @script.start(sevii\floe\daycare\take_back)
                                    :when:No
                                        @text.show(Very well.~Please come again.)
                                :endselect
                            :else
                                @text.show(You don't have enough~money to get~<daycare.pokemonname(2,1)> back.)
                            :endif
                        :else
                            @text.show(You don't have room~in your party!*Come back when you've~made room.)
                        :endif
                    :when:No
                        @text.show(Fine.~Come Again.)
                :endselect

            :when:Cancel
                @text.show(Fine.~Come Again.)
        :endselect
:endselect
:end